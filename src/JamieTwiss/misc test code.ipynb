    "import torch\n",
    "import torch.nn as nn\n",
    "\n",
    "\n",
    "loss = nn.CrossEntropyLoss()\n",
    "input = torch.randn(3, 5, requires_grad=True)\n",
    "target = torch.empty(3, dtype=torch.long).random_(5)\n",
    "print(input)\n",
    "print(input.dtype)\n",
    "output = loss(input, target)\n",
    "output.backward()\n",
    "print(output)"
